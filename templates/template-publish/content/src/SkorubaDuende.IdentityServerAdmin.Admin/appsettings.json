{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AdminConfiguration": {
    "AuthenticationConfiguration": {
      "Authority": "https://localhost:44310",
      "ClientId": "skoruba_identity_admin",
      "ClientSecret": "skoruba_admin_client_secret",
      "AdminScopes": [
        "openid",
        "profile",
        "email",
        "skoruba_identity_admin_api"
      ]
    },
    "ApiConfiguration": {
